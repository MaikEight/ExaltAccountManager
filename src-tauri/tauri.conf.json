    },
    "deep-link": {
      "desktop": {
        "schemes": [
          "eam",
          "exalt-account-manager"
        ]
      }
    }
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "decorations": false,
        "transparent": true,
        "title": "Exalt Account Manager",
