{
  "identifier": "desktop-capability",
  "platforms": [
    "windows",
    "macOS"
  ],
  "windows": [
    "main",
    "local"
  ],
  "permissions": [
    "updater:default",
    "fs:default",
    "shell:default",
    "http:default",
    "process:default",
    "dialog:default",
    "core:event:default",
    "deep-link:default",
    "drpc:allow-is-running",
    "drpc:default",
    "autostart:default",
    "autostart:allow-enable",
    "autostart:allow-disable",
    "autostart:allow-is-enabled",
    "autostart:default"
  ]
}